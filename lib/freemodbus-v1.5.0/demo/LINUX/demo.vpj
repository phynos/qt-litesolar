			</Folder>
			<Folder Name="tcp">
				<F N="../../modbus/tcp/mbtcp.c"/>
				<F N="../../modbus/tcp/mbtcp.h"/>
			</Folder>
			<F N="../../modbus/mb.c"/>
		</Folder>
		<Folder Name="port">
			<F N="port/port.h"/>
			<F N="port/portevent.c"/>
			<F N="port/portother.c"/>
			<F N="port/portserial.c"/>
			<F N="port/porttimer.c"/>
		</Folder>
		<F N="demo.c"/>
	</Files>
</Project>
